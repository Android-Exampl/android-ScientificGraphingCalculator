<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1"
      android:layout_marginBottom="3dp"
      android:layout_marginTop="3dp">
    <TextView
	android:id="@+id/convT1"
	android:textSize="22sp"
	android:layout_height="fill_parent"
	android:layout_width="fill_parent"
	android:layout_gravity="center_vertical|center_horizontal"
	android:textScaleX="1.0"
	android:layout_weight="3.0"
	android:text="fn\u2081(x) = " />
    <view
	class="com.threeDBJ.calcAppLib.NoImeEditText"
	android:id="@+id/fn1"
	android:layout_gravity="center"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:background="@drawable/input"
	android:inputType="none"
	android:padding="5dip"
	android:textSize="25sp" />
  </LinearLayout>
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1"
      android:layout_marginBottom="3dp"
      android:layout_marginTop="3dp">
    <TextView
	android:id="@+id/convT1"
	android:textSize="22sp"
	android:layout_height="fill_parent"
	android:layout_width="fill_parent"
	android:layout_gravity="center_vertical|center_horizontal"
	android:textScaleX="1.0"
	android:layout_weight="3.0"
	android:text="fn\u2082(x) = " />
    <view
	class="com.threeDBJ.calcAppLib.NoImeEditText"
	android:id="@+id/fn2"
	android:layout_gravity="center"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:background="@drawable/input"
	android:inputType="none"
	android:padding="5dip"
	android:textSize="25sp" />
  </LinearLayout>
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1"
      android:layout_marginBottom="3dp"
      android:layout_marginTop="3dp">
    <TextView
	android:id="@+id/convT1"
	android:textSize="22sp"
	android:layout_height="fill_parent"
	android:layout_width="fill_parent"
	android:layout_gravity="center_vertical|center_horizontal"
	android:textScaleX="1.0"
	android:layout_weight="3.0"
	android:text="fn\u2083(x) = " />
    <view
	class="com.threeDBJ.calcAppLib.NoImeEditText"
	android:id="@+id/fn3"
	android:layout_gravity="center"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:background="@drawable/input"
	android:inputType="none"
	android:padding="5dip"
	android:textSize="25sp" />
  </LinearLayout>
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
    <Button
	android:id="@+id/shift"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:text="shift"
	android:background="@drawable/shift1"/>
    <Button
	android:id="@+id/left"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:background="@drawable/left"
	android:text="" />
    <Button
	android:id="@+id/right"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:background="@drawable/right"
	android:text="" />
    <Button
	android:id="@+id/del"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="18sp"
	android:text="bspc" />
    <Button
	android:id="@+id/clr"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="20sp"
	android:text="clr" />
  </LinearLayout>
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
    <Button
	android:id="@+id/ln"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="20sp"
	android:text="ln" />
    <Button
	android:id="@+id/seven"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="0.9"
	android:textSize="20sp"
	android:text="7" />
    <Button
	android:id="@+id/eight"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="0.9"
	android:textSize="20sp"
	android:text="8" />
    <Button
	android:id="@+id/nine"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="0.9"
	android:textSize="20sp"
	android:text="9" />
    <Button
	android:id="@+id/plus"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="21sp"
	android:text="+" />
  </LinearLayout>
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
    <Button
	android:id="@+id/sqr"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="16sp"
	android:text="sqr" />
    <Button
	android:id="@+id/four"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="0.9"
	android:textSize="20sp"
	android:text="4" />
    <Button
	android:id="@+id/five"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="0.9"
	android:textSize="20sp"
	android:text="5" />
    <Button
	android:id="@+id/six"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="0.9"
	android:textSize="20sp"
	android:text="6" />
    <Button
	android:id="@+id/minus"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="21sp"
	android:text="-" />
  </LinearLayout>
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
    <Button
	android:id="@+id/euler"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="20sp"
	android:text="e" />
    <Button
	android:id="@+id/one"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="0.9"
	android:textSize="20sp"
	android:text="1" />
    <Button
	android:id="@+id/two"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="0.9"
	android:textSize="20sp"
	android:text="2" />
    <Button
	android:id="@+id/three"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="0.9"
	android:textSize="20sp"
	android:text="3" />
    <Button
	android:id="@+id/mult"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="21sp"
	android:text="*" />
  </LinearLayout>
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
    <Button
	android:id="@+id/pwr"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="20sp"
	android:text="^" />
    <Button
	android:id="@+id/point"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="22sp"
	android:text="." />
    <Button
	android:id="@+id/zero"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="0.9"
	android:textSize="19sp"
	android:text="0" />
    <Button
	android:id="@+id/sign"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="18sp"
	android:text="(-)" />
    <Button
	android:id="@+id/div"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="21sp"
	android:text="/" />
  </LinearLayout>
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
    <Button
	android:id="@+id/E"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="20sp"
	android:text="E" />
    <Button
	android:id="@+id/lParen"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="18sp"
	android:text="(" />
    <Button
	android:id="@+id/rParen"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="18sp"
	android:text=")" />
    <Button
	android:id="@+id/copy"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1.05"
	android:textSize="14sp"
	android:text="paste" />
    <Button
	android:id="@+id/xVar"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1.05"
	android:textSize="18sp"
	android:text="X" />
  </LinearLayout>
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
    <Button
	android:id="@+id/pie"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="18sp"
	android:text="pi" />
    <Button
	android:id="@+id/sin"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="18sp"
	android:text="sin" />
    <Button
	android:id="@+id/cos"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="18sp"
	android:text="cos" />
    <Button
	android:id="@+id/tan"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_weight="1"
	android:textSize="18sp"
	android:text="tan" />
  </LinearLayout>
  <Button
      android:id="@+id/fns_graph"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1"
      android:textSize="18sp"
      android:text="Graph" />
  <include layout="@layout/ad_space" />
</LinearLayout>
