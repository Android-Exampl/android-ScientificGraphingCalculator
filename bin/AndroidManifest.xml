<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.threeDBJ.calcAppLib"
          android:versionCode="7"
          android:versionName="1.6">
  <uses-sdk
      android:minSdkVersion="7"
      android:targetSdkVersion="9" />
  <application
      android:name="CalcApp"
      android:label="@string/app_name"
      android:icon="@drawable/ic_main">
    <activity
	android:name="com.threeDBJ.calcAppLib.mainTab"
	android:label="@string/app_name"
	android:theme="@android:style/Theme.NoTitleBar"
	android:configChanges="keyboardHidden|orientation">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
	android:name="com.threeDBJ.calcAppLib.mainCalc"
	android:label="@string/app_name"
	android:theme="@android:style/Theme.NoTitleBar"
	android:configChanges="keyboardHidden|orientation">
    </activity>
    <activity
	android:name="com.threeDBJ.calcAppLib.graphCalc"
	android:label="@string/app_name"
	android:theme="@android:style/Theme.NoTitleBar"
	android:configChanges="keyboardHidden|orientation"
	>
    </activity>
    <activity
	android:name="com.threeDBJ.calcAppLib.fnEntry"
	android:label="@string/fn_entry"
	android:configChanges="keyboardHidden|orientation"
	android:theme="@android:style/Theme.NoTitleBar"
	>
    </activity>
    <activity
	android:name="com.threeDBJ.calcAppLib.convCalc"
	android:label="@string/app_name"
	android:theme="@android:style/Theme.NoTitleBar"
	android:configChanges="keyboardHidden|orientation">
    </activity>
  </application>
</manifest>
