/*MAIN*/
import java.io.*;
import cliCalc.*;

public class Main {
    public static void main(String args[]){
	Parser par = new Parser(false);
	BufferedReader in = new BufferedReader(new InputStreamReader(System.in));
	String s;
	try {
	    while ((s = in.readLine()) != null){
		par.run(s);
		System.out.println(par.getError() ? "BAD INPUT TO PARSER" : par.getAns() );
	    }
	}catch (Exception e){
	    System.err.println("ERROR UNDETECTED BY PARSER");
	}
    }
}
